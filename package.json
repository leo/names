{
  "name": "names",
  "version": "1.0.0",
  "description": "Minimal API client for uinames.com",
  "main": "dist/index",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile",
    "test": "eslint src/** test/** && ava -v"
  },
  "repository": "leo/names",
  "eslintConfig": {
    "extends": "default",
    "parser": "babel-eslint"
  },
  "keywords": [
    "uinames",
    "names",
    "random",
    "generator",
    "name"
  ],
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  },
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leo/names/issues"
  },
  "homepage": "https://github.com/leo/names#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-register": "^6.9.0",
    "eslint": "^3.0.0",
    "eslint-config-default": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0"
  },
  "dependencies": {
    "request-promise": "^3.0.0"
  }
}
